<%= simple_form_for(@acta) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fecha_reunion %>
    <%= f.input :tipo_reunion %>
    <%= f.input :lugar %>
    <%= f.cktext_area :orden_dia, rows: 10 %>
    <%= f.cktext_area :desarrollo, rows: 10 %>
    <%= f.cktext_area :tareas, rows: 10 %>
    <%= f.cktext_area :conclusiones, rows: 10 %>
       <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
