<%= simple_form_for(@proyecto) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :titulo %>
   <%= f.cktext_area :descripcion, rows: 10 %>
    <%= f.input :estapa %>
    <%= f.file_field :imagen %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
